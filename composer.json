{
  "name": "monolith/relational-database",
  "description": "Relational database component for the Monolith framework.",
  "keywords": [
    "monolith",
    "http"
  ],
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "vlucas/phpdotenv": "^2.5@dev",
    "monolith/dependency-injection": "^7"
  },
  "require-dev": {
    "phpspec/phpspec": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\RelationalDatabase\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\RelationalDatabase\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
